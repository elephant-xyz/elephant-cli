{
  "name": "@elephant-xyz/cli",
  "version": "1.3.0",
  "description": "CLI tool for Elephant Network",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "elephant-cli": "./bin/elephant-cli"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc -w",
    "test": "mkdir -p ./tmp && TMPDIR=./tmp vitest run --dangerouslyIgnoreUnhandledErrors",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "clean": "rimraf dist",
    "prepare": "husky || true",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "cli",
    "blockchain",
    "polygon",
    "ipfs"
  ],
  "author": "Elephant XYZ",
  "license": "MIT",
  "dependencies": {
    "@ipld/dag-pb": "^4.1.5",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "async-mutex": "^0.5.0",
    "canonicalize": "^2.1.0",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "commander": "^12.0.0",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.5.0",
    "ethers": "^6.14.3",
    "form-data": "^4.0.0",
    "ipfs-unixfs": "^11.2.1",
    "multiformats": "^13.3.6",
    "ora": "^8.2.0",
    "p-limit": "^6.2.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/cli-progress": "^3.11.5",
    "@types/node": "^22.15.30",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitest/coverage-v8": "^3.2.2",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.1.3",
    "hardhat": "^2.24.1",
    "hardhat-ethers": "^1.0.1",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^3.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elephant-xyz/elephant-cli.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin/",
    "dist/",
    "README.md",
    "LICENSE"
  ]
}
